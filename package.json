    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "openai": "^4.20.1",
    "replicate": "^0.22.0",
    "stability-client": "^1.9.0",
